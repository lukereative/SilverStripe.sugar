<?xml version="1.0" encoding="UTF-8"?>
<codesense>
	<provider>
		<selector>other.ss</selector>
		<complete-match>[a-zA-Z0-9_]*</complete-match>
		<completions>com.lukereative.ss.conditions</completions>
		<completions>com.lukereative.ss.control</completions>
		<completions>com.lukereative.ss.methods</completions>
	</provider>
	<provider>
		<selector>condition.start.ss, control.start.ss</selector>
		<complete-match>[a-zA-Z0-9_\.]*</complete-match>
		<completions>com.lukereative.ss.controls</completions>
		<completions>com.lukereative.ss.controls-with-options</completions>
	</provider>
	<provider>
		<selector>requirement.ss</selector>
		<complete-match>[a-zA-Z0-9]*</complete-match>
		<completions>com.lukereative.ss.requirements</completions>
	</provider>
	<provider>
		<selector>variable.ss</selector>
		<complete-match>[a-zA-Z0-9_\.]*</complete-match>
		<completions>com.lukereative.ss.variables</completions>
	</provider>
</codesense>